  <ItemGroup>
    <Protobuf Include="..\GrpcServiceBdVentas\Protos\vendedor.proto" GrpcServices="Client">
      <Link>Protos\vendedor.proto</Link>
    </Protobuf>
  </ItemGroup>

</Project>
